// CLientes Destacados ex. home...


.asidecesped
	margin-top 3em
	.clientessection
		.clienteswrap
			.clientesborder
				border 1px solid verdecesped
				position relative
				text-align center
				padding-bottom 1em
				.h2clientes
					display inline-block
					text-align center
					font-size 1.3em
					color grisOscuro
					font-weight 400
					background white
					padding 1em 2em
					margin-top -2.5em
					.subh2clientes
						padding-top .3em
						color grisClaroOscurecido
						display block
						clear both
				.pclientes
					max-width 80%
					margin 0 auto
					font-size .9em
					color grisClaroOscurecido
					line-height 1.5em
			.imagenes-clientes
				padding-top 2em
				padding-bottom 1em
				position relative
				text-align center
				.imagenes-clientes-figure
					display inline-block
					margin-right .8em
				.imagenes-clientes-figure:last-child
					margin-right 0
				.imagenes-clientes-figure img
					max-width 7em
					hight auto
					overflow hidden




